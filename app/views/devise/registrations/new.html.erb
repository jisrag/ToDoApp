<div class="container">
<div class="row">
<div class="col-md-12" align="center">
<h2>Registrate!</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  

  <div class="field">
    <%= f.label :email, "Correo Electronico" %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :password,"ContraseÃ±a Nueva" %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> minimo)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "new-password" %>
  </div>

  <div class="field">
    <%= f.label :password_confirmation, "Confirmala plis (Igualita)" %><br />
    <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
  </div>
<!--
  <div class="actions">
    <%= f.submit "Resgistrarme" %>
  </div>
  -->
  </br>
  <button type="submit" class="btn btn-primary actions">Registrarme con los chidos</button>
<% end %>

<%= render "devise/shared/links" %>
</div>
</div>
</div>